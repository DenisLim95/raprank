
<% include partials/header %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center">UPLOAD YOUR BEAT</h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/main" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" placeholder="track name" name="name">
                </div>

                <div class="form-group">
                    <input class="form-control" type="text" placeholder="producer name" name="producer">
                </div>
                 
                <div class="form-group">
                    <input class="form-control" type="text" placeholder="image-url" name="image">
                </div>
                
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit</button>    
                </div>
                
            </form>    
            
            <a href="/main">Go Back</a>
        </div>
    </div>
</div>
<% include partials/footer %>

